{
  "name": "@amihhs/amihhs-ui",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.5.1",
  "description": "amihhs-ui",
  "author": {
    "name": "amihhs",
    "email": "amihhs369@gmail.com",
    "url": "https://github.com/amihhs"
  },
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "rimraf packages/*/dist && esno scripts/copy-files.ts && pnpm -r run build",
    "release": "bumpp -r",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "play": "pnpm -C playground run dev",
    "docs:dev": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "docs:preview": "pnpm -C docs run preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/types": "^17.4.4",
    "@types/node": "^20.3.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "esno": "^0.16.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "unocss": "^0.53.4",
    "vitest": "^0.32.2"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json,yml},.eslintignore": "eslint --fix"
  },
  "dependencies": {
    "highlight.js": "^11.8.0"
  }
}
